{
	"_version": "1.7.0",
	"sap.app": {
		"id": "com.magenta_ProdConfirmation",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZPRODCONF_SRV": {
				"uri": "/sap/opu/odata/sap/ZPRODCONF_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "com.magenta_ProdConfirmation.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.collaboration": {},
				"sap.m": {},
				"sap.ui.comp": {},
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.magenta_ProdConfirmation.i18n.i18n"
				}
			},
			"oModel": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZPRODCONF_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewPath": "com.magenta_ProdConfirmation.view",
				"controlId": "app",
				"controlAggregation": "pages"
			},
			"targets": {
				"Selection": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Selection",
					"viewLevel": 1
				},
				"Orders": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Orders",
					"viewLevel": 2
				}
			},
			"routes": [
				{
					"name": "Selection",
					"pattern": "",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"Selection"
					]
				},
				{
					"name": "Orders",
					"pattern": "Orders/{Ordno},{Plant},{Prodline},{Supervisor},{Date}",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"Orders"
					]
				}
			]
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zprodconf/webapp",
		"_version": "1.1.0"
	}
}